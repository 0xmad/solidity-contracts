{
  "name": "hardhat-project",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "coverage": "hardhat coverage",
    "docgen": "hardhat docgen",
    "lint:js": "eslint . --ext=js,ts",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "lint:sol:fix": "npm run lint:sol -- --fix",
    "githook:pre-commit": "cross-env NODE_ENV=production CI=true lint-staged && npm run tsc",
    "githook:pre-push": "cross-env CI=true npm run test",
    "prepare": "is-ci || husky install",
    "tsc": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^8.0.3",
    "@typechain/hardhat": "^3.0.0",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.1",
    "hardhat": "^2.6.8",
    "hardhat-docgen": "^1.2.0",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "lint-staged": "^12.1.1",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "pretty-quick": "^3.1.2",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.17",
    "ts-node": "^10.4.0",
    "typechain": "^6.0.3",
    "typescript": "^4.5.2"
  }
}
